{
  "manifest_version": 2,
  "name": "QR Code Generator for chrome",
  "description": "This will generate QR",
  
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Generate QR"
  },

  "permissions": [
    "activeTab"
  ]
}